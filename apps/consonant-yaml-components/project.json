{
  "name": "consonant-yaml-components",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/consonant-yaml-components/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/consonant-yaml-components"],
      "options": {
        "command": "node apps/consonant-yaml-components/scripts/build.mjs",
        "cwd": "{workspaceRoot}"
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "cd dist/apps/consonant-yaml-components && zip -r ../consonant-yaml-components.zip .",
        "cwd": "{workspaceRoot}"
      }
    }
  },
  "tags": ["scope:figma", "type:app"]
}
