  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --require-config --check .",
    "format:write": "prettier --require-config --write .",
    "build:tokens": "node scripts/build-tokens.js",
    "package:tokens": "npm run build:tokens && node scripts/package-tokens.js && npm pack ./build",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "storybook": "node scripts/prepare-story-data.js && storybook dev -p 6006",
    "build-storybook": "node scripts/prepare-story-data.js && storybook build",
    "sync:figma": "node scripts/sync-figma-variables.js",
    "tokens:clean": "rm -rf tokens build",
    "archive": "git archive --format=zip --output=consonant-2-$(node -p \"require('./package.json').version\").zip HEAD"
  },
